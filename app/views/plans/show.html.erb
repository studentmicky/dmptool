<%- model_class = Plan -%>

<!-- render the project title -->
<%= render :partial => "plan_title", locals: {plan: @plan, plan_data: @plan_data} %>

<!-- render navigation tabs for the project-->
<%= render :partial => "plan_nav_tabs", locals: {plan: @plan, plan_data: @plan_data, active: "details"} %>

<!-- project details -->
<%= render :partial => "plan_details", locals: {plan: @plan, plan_data: @plan_data, selected_guidance_groups: @selected_guidance_groups} %>
